  "description": "this is a simulation of a banking app",
  "main": "server.js",
  "engines": {
    "node": "8.9.4"
  },
  "scripts": {
    "start:dev": "NODE_ENV=development npm-run-all --parallel open:server",
    "heroku-postbuild": "npm run build",
    "start": "NODE_ENV=production babel-node dist/server/server.js",
    "open:server": "nodemon --watch server --exec babel-node -- ./server/server.js",
    "build:webpack": "NODE_ENV=production webpack --config ./webpack.config.prod.babel.js",
    "build:copy": "NODE_ENV=production babel ./ -d dist --copy-files  --ignore node_modules/,dist",
    "build": "npm run build:copy && npm run build:webpack",
    "serve": "node dist/server.js"
  },
